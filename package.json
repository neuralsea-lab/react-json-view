{
    "name": "react-json-view",
    "description": "Interactive react component for displaying javascript arrays and JSON objects.",
    "version": "1.21.1",
    "main": "dist/main.js",
    "dependencies": {
        "flux": "^4.0.1",
        "react-base16-styling": "^0.8.0",
        "react-dnd": "^11.1.3",
        "react-dnd-html5-backend": "^11.1.3",
        "react-lifecycles-compat": "^3.0.4",
        "react-textarea-autosize": "^8.3.1"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.12.17",
        "@babel/plugin-proposal-class-properties": "^7.12.13",
        "@babel/plugin-proposal-decorators": "^7.13.5",
        "@babel/plugin-syntax-class-properties": "^7.12.13",
        "@babel/plugin-syntax-jsx": "^7.12.13",
        "@babel/preset-env": "^7.12.17",
        "@babel/preset-flow": "^7.12.13",
        "@babel/preset-react": "^7.12.13",
        "@babel/register": "7.12.13",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-plugin-react-html-attrs": "^3.0.5",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "chai": "^4.3.0",
        "core-js": "^3.9.0",
        "coveralls": "^3.1.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.2",
        "enzyme": "^3.11.0",
        "eslint": "^7.20.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "html-webpack-plugin": "5.2.0",
        "jsdom": "^16.4.0",
        "mocha": "^8.3.0",
        "moment": "^2.29.1",
        "node-sass": "^5.0.0",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-github-button": "^0.1.11",
        "react-hot-loader": "^4.13.0",
        "react-scripts": "^4.0.2",
        "react-select": "^4.1.0",
        "react-test-renderer": "^17.0.1",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "sinon": "^9.2.4",
        "style-loader": "^2.0.0",
        "webpack": "^5.23.0",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-bundle-size-analyzer": "^3.1.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "scripts": {
        "build": "SET NODE_ENV=production & webpack --config webpack/webpack.config.js -p --display-error-details --progress --optimize-minimize",
        "prebuild": "SET NODE_ENV=test & npm run test:unit",
        "build:demo": "SET NODE_ENV=production & webpack --mode=development --config webpack/webpack.config-demo.js",
        "dev": "SET NODE_ENV=development & webpack serve --config webpack/webpack.config-dev.js --open",
        "modules:debug": "./docker/debug.sh",
        "modules:tree": "webpack --config webpack/webpack.config.js --json ",
        "modules:size-analyzer": "webpack --config webpack/webpack.config.js --json | webpack-bundle-size-analyzer",
        "test": "npm run test:coverage",
        "test:unit": "cross-env NODE_ENV=test nyc mocha test/**/*-test.js",
        "test:coverage": "cross-env NODE_ENV=test & nyc --reporter=text-lcov mocha test/**/*-test.js | coveralls",
        "test:watch": "cross-env NODE_ENV=test nyc mocha -w test/**/*-test.js",
        "lint": "SET NODE_ENV=test & ./node_modules/.bin/eslint src",
        "lint-fixup": "SET NODE_ENV=test & ./node_modules/.bin/eslint src --ext .js,.jsx --fix"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/neuralsea-lab/react-json-view.git"
    },
    "keywords": [
        "array-viewer",
        "base-16",
        "component",
        "interactive",
        "interactive-json",
        "json",
        "json-component",
        "json-display",
        "json-tree",
        "json-view",
        "json-viewer",
        "json-inspector",
        "json-tree",
        "react",
        "react-component",
        "react-dnd",
        "react-json",
        "theme",
        "tree",
        "tree-view",
        "treeview"
    ],
    "license": "MIT",
    "author": "Mac Gainor + Emmett Miller",
    "bugs": {
        "url": "https://github.com/neuralsea-lab/react-json-view/issues"
    },
    "homepage": "https://github.com/neuralsea-lab/react-json-view",
    "directories": {
        "docs": "docs",
        "test": "test"
    }
}
